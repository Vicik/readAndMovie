/* pages/welcome/welcome.wxss */
page{
  height: 100%;
  background-color: #B3D4DB;
}
.container{
  display: flex;
  /* height: 100vh; */
  flex-direction: column;
  align-items: center;
  background-color: #B3D4DB;
}
.user-avatar{
  margin-top: 160rpx;
  width: 200rpx;
  height: 200rpx;
}
.welcome{
  margin-top: 100rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: #000;
}
.start-container{
  margin-top: 200rpx;
  width: 200rpx;
  height: 80rpx;
  border: 1px solid #405f80;
  border-radius: 5px;
  text-align: center;
}
.start{
  font-size: 22rpx;
  font-weight: bold;
  color: #405f80;
  line-height: 80rpx;
}