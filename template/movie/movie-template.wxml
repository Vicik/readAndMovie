<import src="/template/star/star-template.wxml" />
<template name="movie">
  <view class='movie-container' data-movieId='{{movieId}}' catchtap='movieDetail'>
    <image src='{{img}}' class='movie-img'></image>
    <text class='movie-title'>{{title}}</text>
    <template is='star' data="{{rating,stars}}"></template>
  </view>
</template>